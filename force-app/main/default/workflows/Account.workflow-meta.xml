    <rules>
        <fullName>ADDRESS</fullName>
        <actions>
            <name>Billing_City</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Billing_Street</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Shipping_Country</name>
